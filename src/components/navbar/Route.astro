---
import type { RouteDTO } from "../../types/RouteDTO";

interface Props {
  route: RouteDTO;
}
const { route } = Astro.props;
---

<li class="cursor-pointer">
  <a href={route.path} aria-label={route.label}>
    <route.icon
      class="text-text hover:text-primary size-6 transition-colors duration-200 ease-in-out"
    />
  </a>
</li>
